{
  "name": "zoek-bootstrap",
  "version": "0.2.3",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build:patternlab": "gulp patternlab:build",
    "build:cssx": "node-sass --recursive --include-path ./node_modules/normalize-scss/sass --output-style compressed --output ./source/css src/zoek-bootstrap.scss",
    "build:css": "node-sass --recursive --output-style compressed --output ./source/css src/zoek-bootstrap.scss",
    "build:dev": "concurrently --kill-all --names 'Scaffolding,CSS,JS' --prefix '{name}-{time}' --timestamp-format 'HH:mm:ss' 'npm run build:scaffolding-css -- --watch' 'npm run build:css -- --watch' 'npm run build:js -- --watch'",
    "build:js": "rollup --config ./rollup.config.js",
    "build:scaffolding-css": "node-sass --recursive --output-style compressed --output ./source/css pattern-scaffolding/pattern-scaffolding.scss",
    "flow": "flow",
    "gulp": "gulp",
    "install:normalize-scss": "cp -R ./node_modules/normalize-scss/sass/ ./src/normalize-scss",
    "postversion": "git push && git push --tags",
    "start": "gulp patternlab:serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Zoek Applications Ltd",
  "license": "ISC",
  "directories": {
    "bin": "./bin"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-3": "^6.24.1",
    "concurrently": "^3.5.0",
    "domready": "^1.0.8",
    "edition-node-gulp": "^1.4.0",
    "flow-bin": "^0.49.1",
    "flow-typed": "^2.1.2",
    "node-sass": "^4.5.3",
    "normalize-scss": "^7.0.0",
    "patternlab-node": "^2.9.3",
    "rollup": "^0.43.0",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.0.0",
    "starterkit-mustache-base": "^3.0.1",
    "styleguidekit-assets-default": "^3.4.0",
    "styleguidekit-mustache-default": "^3.1.0"
  },
  "dependencies": {
    "fs-extra": "^4.0.0",
    "yargs": "^8.0.2"
  }
}
